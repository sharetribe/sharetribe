  var dataLayer = dataLayer || [];
  function _gtm_identify(dl, community_id, feature_flags){
    if (community_id) {
      dl.push({"event": "identify", "id": "mp-"+community_id+"-admin" });
    }

    dl.push({
      event: 'feature flags',
      featureFlags: feature_flags
    });

  };

